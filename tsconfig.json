{
  "compilerOptions": 
  {
    "module": "CommonJS",
    "esModuleInterop": true,
    "target": "ES2019",
    "allowJs": false,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strict": true,
    "strictFunctionTypes": false,
    "noImplicitThis": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "rootDir": "./",
    "outDir": "build",
    "baseUrl": ".",
    "paths": {
      
    }
  },
  "exclude": [
    "node_modules"
  ]
}